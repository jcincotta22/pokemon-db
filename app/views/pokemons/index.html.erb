<h1>Pokemon DataBase</h1>

<table>
  <thead>
    <tr>
      <th colspan="3" id='header'>All Pokemon</th>
    </tr>
  </thead>

  <tbody>
      <% @pokemons.each do |question| %>
        <tr>
          <td><%= question.name %>
        </tr>
      <% end %>
  </tbody>
</table>


<p>
  <%= link_to 'Add Pokemon to your Pokedex', new_pokemon_path %>
</p><br>
